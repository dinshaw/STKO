<!-- <%$smarty.template%> --><%if $adminUser %><%include file="admin/admin_header.tpl" title="Stockton Outfitters"%><%else%><%include file="header.tpl" title="Stockton Outfitters"%><%/if%><div id="content"><h1 class="top"><%$type%> Reservations <span><%$startDate|date_format:"%B %e"%> - <%$endDate|date_format:"%B %e"%></span></h1><h2><%if $edit eq "true"%>You may edit<%else%>Please enter<%/if%> your registration details below:</h2><%if $error %><p class="error"><%$error|nl2br%></p><%/if%><%if $adminUser %><form name="reservations" action="admin.php" method="post"><input type="hidden" name="mode" value="bookings"><input type="hidden" name="custID" value="<%$custID%>"><input type="hidden" name="resID" value="<%$resID%>"><%else%><form name="reservations" action="index.php" method="post"><input type="hidden" name="mode" value="reserve"><%/if%><input type="hidden" name="action" value="reserve"><table class="userForm"> 			<tr>		<th><span class="error">*</span> First Name</th>		<td><input type="text" name="firstName" value="<%$firstName%>"></td>		<th><span class="error">*</span> Last Name</th>		<td><input type="text" name="lastName" value="<%$lastName%>">		</td>	</tr>		<tr>		<th><span class="error">*</span> Street Address</th>		<td colspan="3"><input type="text" name="address" value="<%$address%>"></td>	</tr>		<tr>		<th>Address line 2</th>		<td><input type="text" name="address2" value="<%$address2%>"></td>		<th><span class="error">*</span> City</th>		<td><input type="text" name="city" value="<%$city%>"></td>	</tr>		<tr>						<th><span class="error">*</span> State</th>			<td><%include file="reserve/statesSelect.tpl" value="$state%></td>			<th><span class="error">*</span> Zip</th>			<td><input type="text" name="zip" value="<%$zip%>"></td>	</tr>		<tr>		<th><span class="error">*</span> Telephone (Home)</th>		<td><input type="text" name="tel1" value="<%$tel1%>"></td>		<th>(Office)</th>		<td><input type="text" name="tel2" value="<%$tel2%>"></td>	</tr>		<tr>		<th>Fax</th>		<td><input type="text" name="fax" value="<%$fax%>"></td>		<th><span class="error">*</span> Email</th>		<td><input type="text" name="email" value="<%$email%>"></td>	</tr></table><p><input name="tc" type="checkbox" value="yes" <%if $tc eq "yes" or $edit eq "true"%>checked<%/if%>> By checking this box you are argeeing to the <a href="index.php?mode=tc" target="_blank" onClick="window.open('index.php?mode=tc','Stockton&nbsp;Outfitters&nbsp;Terms&nbsp;&amp;Conditions','width=600,height=650,scrollbars=yes,resizable=yes');return false;">Terms &amp; Conditions</a> os Stockton Outfitters LLC.</p><hr><!-- REMOVED BY CLIENT REQUEST<h2>Aditional Information</h2><table class="dates"> 			<tr>		<th>Arrival Date</th>		<td><select name="month_arr">			<%include file="reserve/monthSelect.tpl" value=$month_arr%>			</select></td>		<td><select name="day_arr">			<%include file="reserve/daySelect.tpl" value=$day_arr%>			</select></td>		<th>Departure Date</th>		<td><select name="month_dpt">			<%include file="reserve/monthSelect.tpl" value=$month_dpt%>			</select></td>		<td><select name="day_dpt">			<%include file="reserve/daySelect.tpl" value=$day_dpt%>			</select></td>	</tr></table><hr>--><div class="lr"><span><%if $edit eq "true"%><input type="submit" name="save2" value="Save Changes" class="button"><%else%><input type="submit" name="step2" value="Continue" class="button"></span><input type="submit" name="restart" value="Start Over" class="button" onClick="return confirm('Start a new reservation and delete this one?')"><%/if%></div></form></div><%include file="footer.tpl"%>