<!-- <%$smarty.template%> --><%if $adminUser %><%include file="admin/admin_header.tpl" title="Stockton Outfitters"%><%else%><%include file="header.tpl" title="Stockton Outfitters"%><%/if%><div id="content"><h1 class="top"><%$type%><span><%$startDate%> - <%$endDate%></span></h1><%if $adminUser %><h1>admin edit</h1><%/if%><%if $error %><p class="error"><%$error|nl2br%></p><%/if%><table class="userInfo"> 			<tr>		<th colspan="2"><%$firstName%> <%$lastName%></th>	</tr>		<tr>		<th colspan="2"><%$address%></th>	</tr>		<tr>		<th colspan="2"><%$address2%></th>	</tr>		<tr>		<th colspan="2"><%$city%>, <%$state%></th>	</tr>		<tr>		<th colspan="2"><%$zip%></th>	</tr>		<tr>		<th colspan="2"><%$email%></th>	</tr>		<tr>		<td>Telephone (Home)</td>		<th><%$tel1%></th>	</tr>		<%if $tel2 %>	<tr>		<td>Telephone (Office)</td>		<th><%$tel2%></th>	</tr>	<%/if%>		<%if $fax%>	<tr>		<td>Fax</td>		<th><%$fax%></th>	</tr>	<%/if%>	</table><%if $adminUser %><form action="admin.php" method="post"><input type="hidden" name="mode" value="bookings"><%else%><form action="index.php" method="post"><input type="hidden" name="mode" value="reserve"><%/if%><input type="hidden" name="action" value="reserve"><input type="hidden" name="resID" value="<%$resID%>"><input type="hidden" name="custID" value="<%$custID%>"><div class="lr"><span><input type="submit" name="edit2" value="Edit your personal details" class="button"></span></div><h1>Reservation Summary:<span>Confirmation# <%$conf%></span></h1><h5 class="break">Trip type: <span><%$type%></span><h5>Start Date: <span><%$startDate%></span></h5><h5 class="break">End Date: <span><%$endDate%></span></h5><h5>Number of Hunters in party: <span><%$hunters%></span></h5><%if $observers%><h5>Number of non-hunters in party:  <span><%$observers%></span></h5><h5 class="break">Total in party:  <span><%$totalGuests%></span></h5><%/if%><h5><%$priceDesc%></h5><h5 class="break">Price per hunter: <span>$<%$hunterPrice|money_format:2:".":","%></span></h5><%if $observers%><h5 class="break">Price per observer: <span>$<%$observerPrice|money_format:2:".":","%></span></h5><%/if%><div class="lr"><span><input type="submit" name="edit1" value="Edit your trip details" class="button"></span></div></form><hr><h4 class="lr">Total Charges:<span>$<%$totalPrice|money_format:2:".":","%> (usd)</span></h4><h1>Payment by Credit Card</h1><%if $adminUser %><form action="admin.php" method="post"><%else%><form action="index.php" method="post"><%/if%><input type="hidden" name="mode" value="reserve"><input type="hidden" name="action" value="pay"><input type="hidden" name="paymentType" value="cc"><input type="hidden" name="resID" value="<%$resID%>"><input type="hidden" name="totalPrice" value="<%$totalPrice%>"><input type="hidden" name="totalGuests" value="<%$totalGuests%>"><input type="hidden" name="tripID" value="<%$tripID%>"><table class="userForm">	<tr>		<td>Name on card</td>		<td><input type="text" name="ccName" value="<%$ccName%>"></td> 	</tr>		<tr>		<td>Card type</td>		<td><select name="ccType">		<option value="">Choose one...</option>		<option value="visa" <%if $ccType eq "visa"%>selected<%/if%>>Visa</option>		<option value="mc" <%if $ccType eq "mc"%>selected<%/if%>>Master Card</option>		<option value="amex" <%if $ccType eq "amex"%>selected<%/if%>>American Express</option>		<option value="discover" <%if $ccType eq "discover"%>selected<%/if%>>Discover</option>		</select>			</tr>		<tr>		<td>Card Number</td>		<td><input type="text" name="ccNum" value="<%$ccNum%>"></td>	</tr>		<tr>		<td>CVN number</td>		<td><input type="text" name="cvn" value="<%$cvn%>" class="num"></td>	</tr>	<tr>		<td>Amount</td>		<td><input type="text" name="amountPaid" value="<%if $amountPaid >  $minPayment%><%$amountPaid%><%else%><%$minPayment%><%/if%>" class="dollar">		(50 % percent of the total charges is the minimum payment)</td>	</tr>			<tr>		<td colspan="2">		<img src="templates/images/cc/cclogos.jpg" alt="cclogos"></td>	</tr>	</table><div class="lr"><span><input type="submit" name="cc" value="Make Payment" class="button"></span></div><input type="submit" name="success" value="Fake Success" class="button"><input type="submit" name="fail" value="Fake Failure" class="button"></form><h1>Payment by Check</h1><form action="" method="post"><input type="hidden" name="mode" value="reserve"><input type="hidden" name="action" value="pay"><input type="hidden" name="paymentType" value="ck"><input type="hidden" name="resID" value="<%$resID%>"><div class="lr"><span><input type="submit" name="check" value="Pay by Check" class="button"></span></div></form></div><%include file="footer.tpl"%>