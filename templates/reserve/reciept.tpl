<!-- <%$smarty.template%> --><%include file="header.tpl" title="Stockton Outfitters"%><div id="content"><h1 class="top">Thank You!<span>Your Reservation Was Successful!</span></h1><%if $error %><p class="error"><%$error|nl2br%></p><%/if%><h3>Thank you for useing our online reservation system.</h3>	<p>Below are the details of your reservation and your confirmation number.	Please call or email us if you have any questions. One of our representatives will be in touch with you shortly. We look forward to meeting you.</p>		<table class="userInfo"> 		<tr>		<th colspan="2">Confirmation #: <%$conf%></th>	</tr>		<tr>		<th colspan="2"><%$type%></th>	</tr>	<tr>		<th colspan="2"><%$startDate|date_format%> - <%$endDate|date_format%></th>	</tr>	<tr>		<th colspan="2"><%$totalGuests%> Guests (<%$hunters%> Hunters & <%$observers%> Non-Hunting Observers)</th>	</tr>	<tr>		<th colspan="2">Total price: $<%$totalPrice|money_format:2:".":","%></th>	</tr>	<tr>		<th colspan="2">Deposite amount required: $<%$minPayment|money_format:2:".":","%></th>	</tr>	<tr>		<th colspan="2"><hr></th>	</tr>	<tr>		<th colspan="2"><%$firstName%> <%$lastName%></th>	</tr>		<tr>		<th colspan="2"><%$address%></th>	</tr>		<tr>		<th colspan="2"><%$address2%></th>	</tr>		<tr>		<th colspan="2"><%$city%>, <%$state%></th>	</tr>		<tr>		<th colspan="2"><%$zip%></th>	</tr>		<tr>		<th colspan="2"><%$email%></th>	</tr>		<tr>		<td>Telephone (Home)</td>		<th><%$tel1%></th>	</tr>		<%if $tel2 %>	<tr>		<td>Telephone (Office)</td>		<th><%$tel2%></th>	</tr>	<%/if%>		<%if $fax%>	<tr>		<td>Fax</td>		<th><%$fax%></th>	</tr>	<%/if%>	<tr>		<th colspan="2"><hr></th>	</tr>		<tr>		<th colspan="2">Payment by Credit card.</th>	</tr>	<tr>		<th colspan="2">Visa: xxxx xxxx xxxx 12343</th>	</tr>	<tr>		<th colspan="2">Payment by Check.</th>	</tr>	</table><%include file="footer.tpl"%>