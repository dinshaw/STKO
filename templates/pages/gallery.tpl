<!-- <%$smarty.template%> --><%include file="header.tpl" title="Tropy&nbsp;Room&nbsp;-&nbsp;Stockton&nbsp;Outfitters"%><div id="content"><h1 class="top">Trophy Romm &amp; Photo Gallery</h1><form method="post" action="index.php" name="img_cats"><input type="hidden" name="mode" value="gallery"><table>	<tr>		<td><select name="filter"  onChange="this.form.submit()">		<option value="">All</option>		 <% section name=pageList loop=$pageLoop %> 		<option value="<%$pageLoop[pageList].page%>" <%if $filter eq $pageLoop[pageList].page %>selected<%/if%> > <% $pageLoop[pageList].title%></option>		<% /section %>		<option value="M"  <%if $filter eq "M" %>selected<%/if%>>Misc</option>		</select></td>	</tr></table></form> <%section name=imageList loop=$imageLoop%><%if $imageLoop[imageList].newPage && $smarty.section.imageList.index != "0"%><%/if%><%if $imageLoop[imageList].newPage %><h1 class="pic"><%$imageLoop[imageList].newPage%></h1><div><%/if%><%if $imageLoop[imageList].newSubPage%><h2 class="pic"><%$imageLoop[imageList].newSubPage%></h2><%/if%><div class="pic"><span class="title"><%$imageLoop[imageList].title|truncate:15:"...":true%></span><a href="index.php?mode=popup&page=<%$imageLoop[imageList].page%>&id=<%$imageLoop[imageList].id%>" onClick="window.open('index.php?mode=popup&page=<%$imageLoop[imageList].page%>&id=<%$imageLoop[imageList].id%>','Hunting&nbsp;Trophys&nbsp;-&nbsp;Stocktion&nbsp;Outfitters','width=<%if $imageLoop[imageList].w >= 600%><%math equation="x + 50" x=$imageLoop[imageList].w%><%else%>600<%/if%>,height=<%math equation="x + 250" x=$imageLoop[imageList].h%>,scrollbars=yes,resizable=yes'); return false;"><img src="userimages/pages/<%$imageLoop[imageList].page%>/<%$imageLoop[imageList].image%>" width="150"></a></div><%/section%> </div><%include file="footer.tpl"%>