<!-- <%$smarty.template%> --><%include file="admin/admin_header.tpl"%><div id="content"><h1>Details and information for page: <%$title%></h1><p class="error"><%$msg|nl2br%></p><form action="admin.php" method="post" enctype="multipart/form-data"><input type="hidden" name="MAX_FILE_SIZE" value="10490000"><input type="hidden" name="mode" value="details"><input type="hidden" name="action" value="edit"><input type="hidden" name="pageID" value="<%$pageID%>"><table id="details">	<tr>		<td><span>Location:</span> <input type="text" name="location" value="<%$location%>"></td>		<td><span>Season:</span>  <input type="text" name="season" value="<%$season%>"></td>	</tr>	<tr>		<td><span>Trip Duration:</span>  <input type="text" name="duration" value="<%$duration%>"></td>		<td><span>Availability:</span>  <input type="text" name="availability" value="<%$availability%>"></td>	</tr>	<tr>		<td><span>Departs from:</span>  <input type="text" name="departs" value="<%$departs%>"></td>		<td><span>Camp:</span>  <input type="text" name="camp" value="<%$camp%>"></td>	</tr>	<tr>		<td><span>Accommodations:</span>  <input type="text" name="accommodations" value="<%$accommodations%>"></td>		<td><span>Species:</span>  <input type="text" name="species" value="<%$species%>"></td>	</tr>	<tr>		<td colspan="2"><span>Peaks & Passes:</span></td>			</tr>	<tr>		<td colspan="2"><textarea name="peaksPasses" rows="10" cols="80"><%$peaksPasses%></textarea></td>	</tr></table><input type="submit" name="submit" value="Edit"></div><%include file="admin/admin_footer.tpl"%>